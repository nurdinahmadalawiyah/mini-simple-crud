generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Customer {
  no     Int    @id @default(autoincrement())
  nama   String @db.VarChar(50)
  alamat String @db.Text
  kota   String @db.VarChar(50)

  @@map("customers")
}
